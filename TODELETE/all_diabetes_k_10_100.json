{
	"datasets": {
		"Diabetes": {
			"data_dir": "./datasets/diabetes",
			"color_fields": [
				"gender",
				"age",
				"diabetesMed"
			],
			"feature_fields": [
				"num_procedures",
				"num_lab_procedures",
