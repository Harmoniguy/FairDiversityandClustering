			"color": "tab:brown",
			"use_coreset" : true,
			"use_dmax" : true,
			"use_dmin" : true,
			"epsilon" : 0.15,
			"timeout" : 1200
		}
	},
	"parameters": {
		"k": [
			10,
			110,
			10
		],
