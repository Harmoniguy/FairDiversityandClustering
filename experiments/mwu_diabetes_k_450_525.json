{
	"datasets": {
		"Diabetes": {
			"data_dir": "./datasets/diabetes",
			"color_fields": [
				"gender",
				"diabetesMed"
			],
			"feature_fields": [
				"num_procedures",
				"num_lab_procedures",
				"number_outpatient",
				"number_emergency",
				"number_inpatient",
				"number_diagnoses"
			],
			"normalize": true,
			"filter_unique" : true
		}
	},
	"algorithms" : {
		"FMMD-MWU" : {
			"color": "tab:green",
			"use_coreset" : true,
			"use_dmax" : true,
			"use_dmin" : false,
			"mwu_epsilon": 0.75,
			"falloff_epsilon" : 0.15
		}
	},
	"parameters": {
		"k": [
			450,
			525,
			25
		],
		"observations": 5,
		"buildkis_alpha" : 0.1
	}
}