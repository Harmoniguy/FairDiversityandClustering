			"use_dmax" : true,
			"use_dmin" : true,
			"epsilon" : 0.15,
			"timeout" : 7200
		},
		"SFDM-2 (e=.75)" : {
			"alg" : "SFDM-2",
			"color": "#1b5902",
			"marker": "x",
			"use_coreset" : false,
			"use_dmax" : true,
			"use_dmin" : true,
			"epsilon" : 0.75,
			"timeout" : 7200
		}
	},
	"parameters": {
		"k": [
			20,
			101,
			20
		],
		"observations": 10,
		"buildkis_alpha" : 0.1,
		"buildkis_equal_k_js" : true
	}
}
